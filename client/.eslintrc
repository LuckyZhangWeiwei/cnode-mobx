{
    "parser":"babel-eslint",
    "env":{
       "browser":true,
       "es6":true,
       "node":true
    },
    "parserOptions":{
       "ecmaVersion":6,
       "sourceType":"module"
    },
    "extends":"airbnb",
    "rules": {
        "semi": [0],
        "react/jsx-filename-extension":[0],
        "import/extensions": [0],
        "react/prefer-stateless-function":[0],
        "react/prop-types":[0],
        "react/forbid-prop-types":[0],
        "no-nested-ternary":[0],
        "no-confusing-arrow":[0]
    }
}
